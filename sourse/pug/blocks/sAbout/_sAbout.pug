mixin sAbout(data)
	// start sAbout
	+b.SECTION.sAbout.section#sAbout&attributes(attributes)
		.container
			+e.imgWrap
				img.res-i(src="img/svg/mdg_logo_black.svg", alt="")
			+b.section-title.text-center
				h1 О БРЕНДЕ
				
	// end sAbout

mixin sAbout-catalog(data)
	// start sAbout
	+b.SECTION.sAbout--catalog.section#sAbout&attributes(attributes)
		.container.text-center
			+e.name MARINA DE GOLLE
			+b.section-title
				h1 МУЖСКИЕ ИЗДЕЛИЯ ИЗ КОЖИ КРОКОДИЛА
			p &#8470;&nbsp;1&nbsp;в своем деле: непревзойденная экспертиза в&nbsp;работе с&nbsp;кожей крокодила
			